<div>

	<b>Pool:</b> {{name}}<p/>
	<b>Application Entity Title:</b> {{applicationEntityTitle}}<p/>
	<b>Description:</b> {{description}}<p/>

	<table class="table">
		<thead>
			<tr>
				<th>AE Title</th>
				<th>Host</th>
				<th>Port</th>
			</tr>
		</thead>
			{{#each devices}}
			<tr>
				<td>{{this.applicationEntityTitle}}</td>
				<td>{{this.hostName}}</td>
				<td>{{this.port}}</td>
			</tr>
			{{/each}}
	</table>


	{{outlet}}



</div>